# Laravel .gitignore - Professional Configuration

# Environment files (NEVER commit secrets)
.env
.env.backup
.env.production
.env.local
.env.*.local

# Compiled/generated files
/vendor
/node_modules
/public/hot
/public/storage
/storage/*.key

# Laravel compiled files
/bootstrap/cache/*
!.gitkeep

# IDE and editor files
/.idea
/.vscode
*.sublime-project
*.sublime-workspace
*.swp
*.swo
*~

# OS-generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Logs and debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Testing
.phpunit.result.cache
/coverage

# Build output
/public/build
/public/mix-manifest.json

# Homestead/Vagrant
Homestead.json
Homestead.yaml
docker-compose.override.yml
.vagrant/

# Composer
composer.phar

# Auth and tokens (CRITICAL - never commit)
auth.json
*.pem
*.key
!*.blade.php
.credentials
.secrets

# Antigravity/AI assistant
.gemini/
