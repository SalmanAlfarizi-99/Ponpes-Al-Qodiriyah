# Docker ignore file for Laravel
# Excludes unnecessary files from Docker build context

# Git
.git
.gitignore
.gitattributes

# IDE and editors
.idea
.vscode
*.sublime-project
*.sublime-workspace

# Environment files (never include in image)
.env
.env.backup
.env.local
.env.*.local

# Dependencies (will be installed in container)
/vendor
/node_modules

# Development files
/tests
phpunit.xml
.phpunit.result.cache
.styleci.yml

# Documentation
README.md
CHANGELOG.md

# Database dumps
*.sql

# Logs
/storage/logs/*
!/storage/logs/.gitignore

# Cache (will be generated)
/bootstrap/cache/*
!/bootstrap/cache/.gitignore

# Composer files not needed in production
composer.phar
composer-setup.php

# OS files
.DS_Store
Thumbs.db

# Antigravity/AI
.gemini
